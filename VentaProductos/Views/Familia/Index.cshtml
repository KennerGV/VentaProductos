@model List<VentaProductos.Models.ViewModels.Class2>
@{
    ViewBag.Title = "Familia";
}

<h2>@ViewBag.Title</h2>
<div class="row" style="text-align:right;padding:5px;">
    <a href="~/Familia/Nuevo" class="btn btn-primary">Nuevo</a>
</div>
<div class="row">
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>Id Familia</th>
                <th>Nombre Familia</th>
                <th>Usuario Ingreso</th>
                <th>Estado</th>
                @foreach (var oElemento in Model)
                {
                <tr>
                    <td>@oElemento.IDFamilia</td>
                    <td>@oElemento.NombreFamilia</td>
                    <td>@oElemento.UsuarioIngreso</td>
                    <td>@oElemento.Estado</td>
                    <td></td>
                </tr>

                }
                
        </table>
    </div>
</div>
